{% extends 'document.twig' %}

{% block meta %}
    {{parent('meta')}}
    <script src="/Static/default/js/project.edit.js" charset="utf-8"></script>
{% endblock %}

{% block content %}
    <form class="form" action="/project/edit/{{project.id}}/save/" method="post">
        <span id="ProjectEdit"></span>
        <input type="hidden" name="color_edit" value="{{project.color}}">
        <input type="text" name="title" value="{{project.title}}"><br>
        <input type="submit" name="save" value="Save">
    </form>
{% endblock %}
